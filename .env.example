# Metanalyst Agent Configuration

# Anthropic API Key (default LLM provider)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Tavily API Key (default search provider)
TAVILY_API_KEY=your_tavily_api_key_here

# OpenAI API Key (for embeddings)
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=metanalyst
DB_USER=postgres
DB_PASSWORD=your_postgres_password

# Alternative: Full database URL
# DATABASE_URL=postgresql://postgres:password@localhost:5432/metanalyst

# Debug Mode
DEBUG=false

# Optional: Custom model configurations
# ORCHESTRATOR_MODEL=claude-3-5-sonnet-latest
# RESEARCHER_MODEL=claude-3-5-sonnet-latest
# PROCESSOR_MODEL=claude-3-5-sonnet-latest